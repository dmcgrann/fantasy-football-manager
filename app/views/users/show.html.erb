<h3>Welcome, <%=@user.name.upcase%>!</h3>
<h4>Teams</h4>
<ol>
<% @user.teams.each do |team| %>
  <li><%=link_to team.team_name, user_team_path(@user, team)%><br>
    <button><%= link_to "Edit Team Info", edit_user_team_path(@user, team) %></button><br>
    <%= form_tag team_path(team), method: "delete" do %>
      <%= submit_tag "Delete" %>
      <%end%>

  </li>
<%end%>
</ol>

<p><%= link_to "Create a New Team", new_user_team_path(@user) %></p>
