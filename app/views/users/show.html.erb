<h3>Welcome, <%=@user.name.upcase%>!</h3>
<h4>Teams</h4>
<ol>
<% @user.teams.each do |team| %>
  <li><%=link_to team.name, team_path(team)%></li>
<%end%>
</ol>

<p><%= link_to "Create a New Team", new_team_path %></p>
<% if current_user.admin %>
<p><%= link_to "Create a New League", new_league_path %></p>
<%end%>
