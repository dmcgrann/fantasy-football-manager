<h2><%= @team.team_name %></h2>
<h3><%= link_to @team.league.name, leagues_path %></h3>
<ul>
<h4>QBs</h4>
<% @team.players.qb.each do |player| %>
  <%= player.player_name%>, <%=player.nfl_team %><br>
<%end%>

<h4>RBs</h4>
<% @team.players.rb.each do |player| %>
  <%= player.player_name%>, <%=player.nfl_team %><br>
<%end%>

<h4>WRs</h4>
<% @team.players.wr.each do |player| %>
  <%= player.player_name%>, <%=player.nfl_team %><br>
<%end%>

<h4>TEs</h4>
<% @team.players.te.each do |player| %>
  <%= player.player_name%>, <%=player.nfl_team %><br>
<%end%>
</ul>
