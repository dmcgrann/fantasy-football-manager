<h3> Edit Your Team</h3>

<%= form_for @team do |f| %>
  <div class="form-group">
  <p><label>Team Name</label><br>
  <%= f.text_field :name %></p>

  <label>Select Players to Add</label><br>
  <p><%= f.collection_select :player_ids, Player.all, :id, :player_info, {:prompt => "Please Select Your Players"}, {:multiple => true} %></p>

  <%= f.hidden_field :user_id, value: current_user.id  %>

  <%= f.submit "Submit" %>
  </div>
<%end%>
